    "1aeb6e76-a0ac-4156-8d2e-1e273bb5722f": {
      "type": "rich-text",
      "blocks": {
        "c51a6d3f-d089-4566-b101-c136cd33ff32": {
          "type": "text",
          "settings": {
            "text": "<p>Diese Prinzipien verankern Uncle Rusty’s im Herzen der malerischen bayerischen Landschaft - wo unsere Menüs entstehen. Doch wie zeichnen sich unsere Menüs aus? <\/p>"
          }
        }
      },
      "block_order": [
        "c51a6d3f-d089-4566-b101-c136cd33ff32"
      ],
      "settings": {
