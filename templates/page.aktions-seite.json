    "1203cbb7-8bb0-4313-97dd-db7a4e77a5e8": {
      "type": "ur-image-with-text",
      "blocks": {
        "8ce866ce-9b7c-44f0-96b5-164553484e4d": {
          "type": "heading",
          "settings": {
