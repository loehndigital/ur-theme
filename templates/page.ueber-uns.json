    "46b98696-f663-4732-816d-9b2021134506": {
      "type": "ur-image-with-text",
      "blocks": {
        "template--21410078720295__46b98696-f663-4732-816d-9b2021134506-heading-1": {
          "type": "heading",
          "settings": {
            "heading": "<strong>Über Uns<\/strong>",
            "heading_size": "h1"
          }
        },
        "template--21410078720295__46b98696-f663-4732-816d-9b2021134506-text-1": {
          "type": "text",
          "settings": {
            "text": "<p>Wir sind ein familiäres, ambitioniertes Unternehmen aus dem malerischen Süden Bayerns, das sich der Bereitstellung von Premium-Hundefutter verschrieben hat. Wir bieten deinem vierbeinigen Freund nur das Beste, denn Uncle Rusty's ist nicht nur eine Marke, sondern eine Herzensangelegenheit.<\/p>",
            "text_style": "body"
          }
        }
      },
      "block_order": [
        "template--21410078720295__46b98696-f663-4732-816d-9b2021134506-heading-1",
        "template--21410078720295__46b98696-f663-4732-816d-9b2021134506-text-1"
      ],
      "settings": {
        "image": "shopify:\/\/shop_images\/michi_1_be69c882-0f70-4bf4-b443-e01c467b73d9.jpg",
        "height": "medium",
        "image_crop": "crop",
        "rounded_corners": "not-rounded",
        "desktop_image_width": "medium",
